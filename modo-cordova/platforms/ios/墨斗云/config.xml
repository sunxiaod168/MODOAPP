<?xml version='1.0' encoding='utf-8'?>
<widget id="com.modencloud.modoapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="ios-package" value="CDVSplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
    </feature>
    <feature name="FileOpener2">
        <param name="ios-package" value="FileOpener2" />
    </feature>
    <hook src="hooks/hookers.js" type="before_prepare" />
    <hook src="hooks/beforedep.js" type="before_deploy" />
    <name>墨斗云</name>
    <description>墨斗云家居建材零售行业销售管理平台</description>
    <author email="sunxiaod168@hotmail.com" href="http://www.modencloud.com">Modou Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="src/assets/images/ios/Icon-180.png" width="180" />
    <icon height="60" src="src/assets/images/ios/Icon-60.png" width="60" />
    <icon height="120" src="src/assets/images/ios/Icon-120.png" width="120" />
    <icon height="76" src="src/assets/images/ios/Icon-76.png" width="76" />
    <icon height="152" src="src/assets/images/ios/Icon-152.png" width="152" />
    <icon height="40" src="src/assets/images/ios/Icon-40.png" width="40" />
    <icon height="80" src="src/assets/images/ios/Icon-80.png" width="80" />
    <icon height="57" src="src/assets/images/ios/Icon-57.png" width="57" />
    <icon height="114" src="src/assets/images/ios/Icon-114.png" width="114" />
    <icon height="72" src="src/assets/images/ios/Icon-72.png" width="72" />
    <icon height="144" src="src/assets/images/ios/Icon-144.png" width="144" />
    <icon height="29" src="src/assets/images/ios/Icon-29.png" width="29" />
    <icon height="58" src="src/assets/images/ios/Icon-58.png" width="58" />
    <icon height="50" src="src/assets/images/ios/Icon-50.png" width="50" />
    <icon height="100" src="src/assets/images/ios/Icon-100.png" width="100" />
    <icon height="167" src="src/assets/images/ios/Icon-167.png" width="167" />
    <splash height="480" src="src/assets/images/launch-320-480.png" width="320" />
    <splash height="960" src="src/assets/images/launch-640-960.png" width="640" />
    <splash height="1024" src="src/assets/images/launch-768-1024.png" width="768" />
    <splash height="2048" src="src/assets/images/launch-1536-2048.png" width="1536" />
    <splash height="1136" src="src/assets/images/launch-640-1136.png" width="640" />
    <splash height="1334" src="src/assets/images/launch-750-1334.png" width="750" />
    <splash height="2208" src="src/assets/images/launch-1242-2208.png" width="1242" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
</widget>
